<UserControl x:Class="QIQO.Client.Controls.BaseTile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QIQO.Client.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="100" BorderBrush="Black" BorderThickness="1">
    <StackPanel>
        <Label Content="{Binding TileLabel}"/>
        <Label Content="{Binding TileValue}" FontSize="20" FontWeight="DemiBold"/>
    </StackPanel>
</UserControl>
